<!DOCTYPE html>
<html>
  <head>
    <title>Welcome to User List</title>
    <link rel="stylesheet" type="text/css" href="/styles.css" />
  </head>
  <body>
    <h1>User List</h1>
    <h2>Here are the users that have registered:</h2>
    <table border="1">
      <thead>
        <tr>
          <th>Username</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/">Back to Home</a>
  </body>
</html>
